{
  "name": "avm",
  "version": "0.1.0",
  "description": "WIP",
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/miswanting/avm.git"
  },
  "keywords": [
    "avm"
  ],
  "author": "Miswanting",
  "license": "GPL-3.0+",
  "scripts": {
    "init": "pnpm install",
    "update": "pnpm update",
    "build": "pnpm webpack",
    "watch": "pnpm webpack --watch",
    "run": "chcp 65001 && pnpm electron .",
    "docs:dev": "pnpm vitepress dev docs",
    "docs:build": "pnpm vitepress build docs",
    "docs:serve": "pnpm vitepress serve docs",
    "pack": "pnpm electron-builder --dir",
    "dist": "pnpm electron-builder",
    "remove": "Remove-Item -Path yarn.lock && Remove-Item -Path node_modules -Recurse && Remove-Item -Path dist -Recurse",
    "clean:yarn": "pnpm cache clean --all",
    "clean:npm": "npm cache clean --force"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "next",
    "@iarna/toml": "latest",
    "@vueuse/core": "latest",
    "@vueuse/electron": "latest",
    "cheerio": "latest",
    "http-proxy-agent": "latest",
    "js-yaml": "latest",
    "lodash": "latest",
    "nanoid": "latest",
    "node-fetch": "latest",
    "sanitize.css": "latest",
    "socks-proxy-agent": "latest",
    "vue": "next",
    "vue-i18n": "next",
    "vue-router": "next",
    "vuex": "next",
    "xml2js": "latest"
  },
  "devDependencies": {
    "@types/js-yaml": "latest",
    "@types/lodash": "latest",
    "@types/tmp": "latest",
    "@vue/compiler-sfc": "latest",
    "css-loader": "latest",
    "electron": "latest",
    "html-webpack-plugin": "latest",
    "pug": "latest",
    "pug-plain-loader": "latest",
    "style-loader": "latest",
    "stylus": "latest",
    "stylus-loader": "latest",
    "tmp": "latest",
    "ts-loader": "latest",
    "typescript": "latest",
    "vue-loader": "next",
    "vue-style-loader": "latest",
    "webpack": "latest",
    "webpack-cli": "latest",
    "yaml-loader": "latest"
  },
  "build": {
    "appId": "net.erajs.avm",
    "directories": {
      "output": "build"
    }
  }
}