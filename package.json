{
  "name": "avm",
  "version": "0.1.0",
  "main": "dist/main.js",
  "repository": "https://github.com/miswanting/avm.git",
  "author": "Miswanting",
  "license": "GPL-3.0+",
  "scripts": {
    "init": "pnpm install",
    "build": "pnpm webpack",
    "watch": "pnpm webpack --watch",
    "start": "chcp 65001 && pnpm electron .",
    "docs:dev": "pnpm vitepress dev docs",
    "docs:build": "pnpm vitepress build docs",
    "docs:serve": "pnpm vitepress serve docs",
    "pack": "pnpm electron-builder --dir",
    "dist": "pnpm electron-builder",
    "remove": "Remove-Item -Path yarn.lock && Remove-Item -Path node_modules -Recurse && Remove-Item -Path dist -Recurse",
    "clean:yarn": "pnpm cache clean --all",
    "clean:npm": "npm cache clean --force"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "vue-i18n": "next"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@iarna/toml": "^2.2.5",
    "@types/js-yaml": "^4.0.5",
    "@vue/compiler-sfc": "^3.2.24",
    "bulma": "^0.9.3",
    "crawler": "^1.3.0",
    "css-loader": "^6.5.1",
    "electron": "^16.0.4",
    "electron-builder": "^22.14.5",
    "html-webpack-plugin": "^5.5.0",
    "jimp": "^0.16.1",
    "nanoid": "^3.1.30",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "sanitize.css": "^13.0.0",
    "style-loader": "^3.3.1",
    "stylus": "^0.55.0",
    "stylus-loader": "^6.2.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.3",
    "vitepress": "^0.19.2",
    "vue": "^3.2.24",
    "vue-loader": "^16.8.3",
    "vue-router": "^4.0.12",
    "vue-style-loader": "^4.1.3",
    "vuex": "^4.0.2",
    "webpack": "^5.65.0",
    "webpack-cli": "latest"
  },
  "build": {
    "appId": "net.erajs.avm",
    "directories": {
      "output": "build"
    }
  }
}